on early-init
    mount debugfs debugfs /sys/kernel/debug
    insmod /sbin/qcrypto_module.ko
    insmod /sbin/qdrbg_module.ko

on init

    export LD_PRELOAD "libboringssl-compat.so"

    export LD_SHIM_LIBS "/system/vendor/lib/libmmcamera2_sensor_modules.so|libshim_camera.so:/system/lib/hw/camera.vendor.msm8952.so|libshim_camera.so:/system/lib64/libnubiajpeg.so|libshim_camera_app.so:/system/lib/libnubiajpeg.so|libshim_camera_app.so:/system/lib64/libAlgorithm.so|libshim_camera_app.so"

    start qfintverify
    # Set permissions for persist partition
    mkdir /persist 0771 system system

    # Support legacy paths
    symlink /sdcard /mnt/sdcard
    symlink /sdcard /storage/sdcard0

on early-boot
    # set RLIMIT_MEMLOCK to 64MB
    setrlimit 8 67108864 67108864
    # Allow subsystem (modem etc) debugging
    write /sys/module/subsystem_restart/parameters/enable_debug ${persist.sys.ssr.enable_debug}
    write /sys/module/pil_msa/parameters/pbl_mba_boot_timeout_ms ${persist.sys.mba_boot_timeout}
    write /sys/module/pil_msa/parameters/modem_auth_timeout_ms ${persist.sys.modem_auth_timeout}
    write /sys/module/pil_msa/parameters/modem_dbg_cfg ${persist.sys.modem_dbg_cfg}
    write /sys/module/peripheral_loader/parameters/proxy_timeout_ms ${persist.sys.pil_proxy_timeout}
    write /sys/kernel/codec_type/boot ${persist.sys.external}
    write /sys/kernel/boot_adsp/boot 1
    chown root audio /sys/kernel/boot_adsp/boot

    exec -- /init.qcom.early_boot.sh

